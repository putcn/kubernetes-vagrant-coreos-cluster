{
    "kind": "ReplicaSet",
    "spec": {
        "template": {
            "spec": {
                "containers": [
                    {
                        "name": "paddlejob",
                        "image": "putcn/cloud-job",
                        "volumeMounts": [],
                        "command": [
                            "paddle_k8s",
                            "start_new_pserver"
                        ],
                        "env": [
                            {
                                "name": "PADDLE_JOB_NAME",
                                "value": "paddlejob"
                            },
                            {
                                "name": "TRAINERS",
                                "value": "2"
                            },
                            {
                                "name": "PSERVERS",
                                "value": "2"
                            },
                            {
                                "name": "TOPOLOGY",
                                "value": "trainer_config.py"
                            },
                            {
                                "name": "ENTRY",
                                "value": "python train.py"
                            },
                            {
                                "name": "TRAINER_PACKAGE",
                                "value": "/trainings/fit_a_line"
                            },
                            {
                                "name": "PADDLE_INIT_PORT",
                                "value": "7164"
                            },
                            {
                                "name": "PADDLE_INIT_TRAINER_COUNT",
                                "value": "1"
                            },
                            {
                                "name": "PADDLE_INIT_PORTS_NUM",
                                "value": "1"
                            },
                            {
                                "name": "PADDLE_INIT_PORTS_NUM_FOR_SPARSE",
                                "value": "1"
                            },
                            {
                                "name": "PADDLE_INIT_NUM_GRADIENT_SERVERS",
                                "value": "2"
                            },
                            {
                                "name": "PADDLE_INIT_NUM_PASSES",
                                "value": "1"
                            },
                            {
                                "name": "PADDLE_INIT_USE_GPU",
                                "value": "0"
                            },
                            {
                                "name": "NUM_PSERVERS",
                                "value": "2"
                            },
                            {
                                "valueFrom": {
                                    "fieldRef": {
                                        "fieldPath": "metadata.namespace"
                                    }
                                },
                                "name": "NAMESPACE"
                            }
                        ],
                        "ports": [
                            {
                                "containerPort": 7164,
                                "name": "jobport-0"
                            },
                            {
                                "containerPort": 7165,
                                "name": "jobport-1"
                            }
                        ],
                        "resources": {
                            "requests": {
                                "cpu": "1",
                                "memory": "1Gi"
                            },
                            "limits": {
                                "cpu": "1.5",
                                "memory": "1Gi"
                            }
                        }
                    }
                ],
                "volumes": []
            },
            "metadata": {
                "labels": {
                    "paddle-job-pserver": "paddlejob",
                    "jobname": "paddlejob"
                }
            }
        },
        "replicas": 2
    },
    "apiVersion": "extensions/v1beta1",
    "metadata": {
        "name": "paddlejob-pserver"
    }
}